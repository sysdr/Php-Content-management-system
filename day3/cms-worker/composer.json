{
    "name": "high-scale-cms/worker",
    "description": "High-Scale PHP CMS Worker powered by RoadRunner (Day 3).",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "spiral/roadrunner-http": "^3.0",
        "nyholm/psr7": "^1.8",
        "spiral/roadrunner-cli": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
